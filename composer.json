{
    "name": "xippogmbh/boot24-bundle",
    "type": "contao-bundle",
    "description": "OSM Bundle for contao 4",
    "homepage": "https://github.com/xippoGmbH/contao-boot24-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Aurelio Gisler",
            "homepage": "https://github.com/xippoGmbH"
        },
        {
            "name": "Contao Community",
            "homepage": "https://contao.org/contributors"
        }
    ],
    "require": {
        "php": "^7.0",
        "contao/core-bundle": "^4.9",
        "symfony/framework-bundle": "^4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "XippoGmbH\\Boot24Bundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "XippoGmbH\\Boot24Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XippoGmbH\\Boot24Bundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/xippoGmbH/contao-boot24-bundle/issues",
        "source": "https://github.com/xippoGmbH/contao-boot24-bundle"
    }
}
